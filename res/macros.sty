\NeedsTeXFormat{LaTeX2e}
\ProvidesPackage{macros}[2023/05/05 Pacote de macros para carregar tabelas]

 \pgfplotstableset
 {% global config, for example in the preamble
  every head row/.style={before row=\toprule,after row=\midrule},
  every last row/.style={after row=\bottomrule},
}
\pgfplotsset{compat=1.18}

% ,TEnd_Medio,N_Iter_Medio,N_Evals_F_Medio,Erro_Xsol_P_Medio,Erro_F_P_Medio,Erro_Xsol_P

\newcommand{\writetable}[1]
{  
    \pgfplotstabletypeset
    [
        columns/Metodos/.style={string type, column name={Método}},
        columns/TEnd_Medio/.style={column name={Tempo (s)} ,sci,sci zerofill,sci sep align,precision=2,sci superscript},
        columns/N_Iter_Medio/.style={column name={Iterações}},
        columns/N_Evals_F_Medio/.style={column name={Avaliações de F(x)}},
        columns/Erro_Xsol_P_Medio/.style={column name={Erro $X_{sol}$ (\%)},sci,sci zerofill,sci sep align,precision=2,sci superscript},
        columns/Erro_F_P_Medio/.style={column name={Erro $F(x_{sol})$ (\%)},sci,sci zerofill,sci sep align,precision=2,sci superscript},        
    ]{#1}  
 }

